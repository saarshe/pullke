{
  "name": "@pullke/alfred",
  "version": "1.0.0",
  "description": "Alfred workflow for pullke core functionality",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist build",
    "type-check": "tsc --noEmit",
    "lint": "prettier --write .",
    "lint:check": "prettier --check .",
    "build:workflow": "node ./scripts/build-workflow.mjs"
  },
  "dependencies": {
    "@pullke/core": "workspace:*"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/node": "^20.0.0",
    "archiver": "^7.0.1",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "workflow",
    "scripts",
    "build"
  ],
  "keywords": [
    "alfred",
    "workflow",
    "github",
    "pullke"
  ],
  "author": "Saar Shechter",
  "license": "MIT"
}
