{
  "name": "@pullke/core",
  "version": "1.0.0",
  "description": "Core functionality for searching GitHub repositories with caching and authentication",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./scripts/*": {
      "import": "./dist/scripts/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "prettier --write .",
    "lint:check": "prettier --check ."
  },
  "dependencies": {
    "@octokit/rest": "^20.0.0",
    "@octokit/types": "^14.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "github",
    "repositories",
    "search",
    "cli",
    "alfred"
  ],
  "author": "Saar Shechter",
  "license": "MIT"
}
